{
  "name": "tdx-api",
  "version": "1.0.0",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:ui": "vitest --ui",
    "clean": "rimraf .turbo node_modules build",
    "lint": "run-p lint:style lint:type",
    "lint:style": "eslint --ext .js,.ts src",
    "lint:type": "tsc --noEmit",
    "lint-fix": "pnpm lint:style --fix",
    "openapi:typegen": "npx openapi-typescript src/docs/*.json --output ./ && prettier --write 'src/docs/*'",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@vitest/ui": "^0.29.7",
    "eslint": "^8.12.0",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "tsx": "^3.12.6",
    "typescript": "^5.0.2",
    "vite": "^4.1.2",
    "vitest": "^0.28.5"
  }
}
